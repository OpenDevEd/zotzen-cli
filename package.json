  "bugs": {
    "url": "https://github.com/opendeved/zotzen-cli/issues"
  },
  "homepage": "https://github.com/opendeved/zotzen-cli#readme",
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.21.1",
    "opn": "^6.0.0",
    "prompt": "^1.0.0",
    "zenodo-lib": "^1.0.2",
    "zotzen-lib": "^1.0.10"
  }
}
