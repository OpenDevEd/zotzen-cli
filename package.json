{
  "name": "zotzen-cli",
  "version": "1.0.1",
  "description": "Command line utility to move data between Zotero and Zenodo",
  "main": "zotzen-cli.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node zotzen-cli.js",
    "prepare": "echo \"prepare\"",
    "version:old": "git add -A src",
